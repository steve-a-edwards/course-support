<?xml version="1.0" ?>
<!--
Request the files in a specific domain, in a specific folder.
The folder must be one of the built-in top-level folders.
 -->
<S:Envelope xmlns:S="http://schemas.xmlsoap.org/soap/envelope/">
        <S:Body>
                <ns2:request
                        xmlns:ns2="http://www.datapower.com/schemas/management"
                        domain="GatewayScript-Examples">
                        <ns2:get-filestore location="logtemp:" />
                </ns2:request>
        </S:Body>
</S:Envelope>


